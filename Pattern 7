#include <iostream>

using namespace std;

int main()
{
    int n,count=1;
    cin>>n;
    int row=1;
    while(row<=n){
        int col=1;
        int val=row;
        while(col<=row){
            cout<<val;
            val=val+1;
            col=col+1;
        }
        cout<<endl;
        row=row+1;
    }
    
    
    
    
   Input: 5
   Output:
   1
   23
   345
   4567
   56789
   
