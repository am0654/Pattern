#include <iostream>

using namespace std;

int main()
{
    int n,val;
    cin>>n;
    int row=1;
    while(row<=n){
        int col=1;
        while(col<=row){
            for(val=row;val>=1;val--){
                cout<<" "<<val;
                col=col+1;
            }
        }
        cout<<endl;
        row=row+1;
    }
   
}



Input:5
1
2 1
3 2 1
4 3 2 1
5 4 3 2 1
